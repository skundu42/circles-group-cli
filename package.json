{
  "name": "circles-groups-cli",
  "version": "1.0.0",
  "description": "A comprehensive CLI for managing Circles groups with deployment, member management, and trust operations",
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "cg": "./dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skundu42/circles-groups-cli.git"
  },
  "homepage": "https://github.com/skundu42/circles-groups-cli#readme",
  "bugs": {
    "url": "https://github.com/skundu42/circles-groups-cli/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx src/index.ts",
    "start": "node dist/src/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:smoke": "vitest run tests/smoke",
    "test:unit": "vitest run tests/unit",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit --skipLibCheck --allowJs",
    "ci": "npm run lint:check && npm run format:check && npm run type-check && npm run test:run",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "postinstall": "chmod +x dist/index.js"
  },
  "dependencies": {
    "@circles-sdk/abi-v2": "^0.29.1",
    "@circles-sdk/adapter-ethers": "^0.29.1",
    "@circles-sdk/sdk": "^0.29.1",
    "@circles-sdk/utils": "^0.29.1",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.8.0",
    "@types/node-fetch": "^2.6.13",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "circles",
    "cli",
    "groups",
    "trust",
    "deployment"
  ]
}
